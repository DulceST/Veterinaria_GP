generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TipoMascota{
  idTipo Int @id    @default(autoincrement())
  Tipo   String
  foto   String
  mascotas Mascota[]
}
 model Mascota{
  id  Int @id   @default(autoincrement())
  nombre String
  sexo  String
  raza  String
  nacimiento  DateTime
  color String
  senasPaerticulares  String
  tipomascotaId Int 
  tipomascota TipoMascota @relation(fields: [tipomascotaId], references: [idTipo])
 }
